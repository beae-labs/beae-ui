{
  "name": "@beae-ui/number-utils",
  "version": "1.0.0",
  "description": "Beae UI | Number attributes function for building components",
  "keywords": [
    "number-utils"
  ],
  "author": "Albert Tran <trantoan.fox.97@gmail.com>",
  "homepage": "https://github.com/beae-labs/beae-ui#readme",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beae-labs/beae-ui.git",
    "directory": "packages/utilities/number-utils"
  },
  "bugs": {
    "url": "https://github.com/beae-labs/beae-ui/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "devDependencies": {
    "clean-package": "2.1.1"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
