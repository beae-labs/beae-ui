{
  "name": "@beae-ui/dropdown",
  "version": "1.0.0",
  "description": "Beae UI | make component dropdown",
  "author": "Toan Tran (Albert) <trantoan.fox.97@gmail.com>",
  "homepage": "https://github.com/beae-labs/beae-ui#readme",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beae-labs/beae-ui.git",
    "directory": "packages/components/dropdown"
  },
  "bugs": {
    "url": "https://github.com/beae-labs/beae-ui/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@beae-ui/system": "workspace:^",
    "vue": "^3.3.4",
    "@beae-ui/utils": "workspace:^",
    "@beae-ui/composables": "workspace:^",
    "@beae-ui/clickable": "workspace:^",
    "@beae-ui/descendant": "workspace:^",
    "@beae-ui/react-use-focus-effect": "workspace:^",
    "@beae-ui/popper": "workspace:^",
    "@beae-ui/react-use-disclosure": "workspace:^",
    "@beae-ui/react-use-outside-click": "workspace:^",
    "@beae-ui/react-use-animation-state": "workspace:^",
    "@beae-ui/react-context": "workspace:^",
    "@beae-ui/react-children-utils": "workspace:^",
    "@beae-ui/react-use-controllable-state": "workspace:^",
    "@beae-ui/react-use-update-effect": "workspace:^",
    "@beae-ui/react-use-merge-refs": "workspace:^",
    "@beae-ui/shared-utils": "workspace:^",
    "@beae-ui/lazy-utils": "workspace:^"
  },
  "devDependencies": {
    "clean-package": "2.2.0"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
